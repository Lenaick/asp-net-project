@model Nullable<System.Boolean>
@{var fieldName = ViewData.TemplateInfo.HtmlFieldPrefix;}
@if (@Model == true)
{
    <input type="checkbox" checked data-toggle="toggle" name="@fieldName" id="@fieldName" value="true">
}
else
{
    <input type="checkbox" data-toggle="toggle" name="@fieldName" id="@fieldName" value="true">
}
<script>
    $(function () {
        $('#toggle-event').change(function () {
            $('#console-event').html('Toggle: ' + $(this).prop('checked'))
        })
    })
</script>